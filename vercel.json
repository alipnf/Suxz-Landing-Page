{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*).(js|css|png|jpg|jpeg|svg)",
      "dest": "/$1.$2"
    },
    {
      "src": "/(favicon.ico)",
      "dest": "/$1"
    },
    {
      "src": ".*",
      "dest": "index.html"
    }
  ]
}
